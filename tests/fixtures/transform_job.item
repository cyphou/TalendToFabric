<?xml version="1.0" encoding="UTF-8"?>
<!-- Fixture: Transform job — tOracleInput → tMap → tFilterRow → tPostgresqlOutput -->
<talendfile:ProcessType xmlns:talendfile="platform:/resource/org.talend.model/model/TalendFile.xsd"
                        xmlns:xmi="http://www.omg.org/XMI">
  <node componentName="tOracleInput" componentVersion="0.102">
    <elementParameter name="UNIQUE_NAME" value="tOracleInput_1"/>
    <elementParameter name="HOST" value="oracle-server"/>
    <elementParameter name="DBNAME" value="ORCL"/>
    <elementParameter name="TABLE" value="ORDERS"/>
    <elementParameter name="QUERY" value="SELECT * FROM SALES.ORDERS WHERE STATUS = 'ACTIVE'"/>
  </node>
  <node componentName="tMap" componentVersion="0.102">
    <elementParameter name="UNIQUE_NAME" value="tMap_1"/>
    <elementParameter name="EXPRESSION_total" value="row1.quantity * row1.unit_price"/>
    <elementParameter name="MAP_full_name" value="StringHandling.UPCASE(row1.first_name) + ' ' + row1.last_name"/>
  </node>
  <node componentName="tFilterRow" componentVersion="0.102">
    <elementParameter name="UNIQUE_NAME" value="tFilterRow_1"/>
    <elementParameter name="CONDITION" value="row2.total > 100"/>
    <elementParameter name="FILTER_TYPE" value="ADVANCED"/>
  </node>
  <node componentName="tPostgresqlOutput" componentVersion="0.102">
    <elementParameter name="UNIQUE_NAME" value="tPostgresqlOutput_1"/>
    <elementParameter name="HOST" value="pg-server"/>
    <elementParameter name="DBNAME" value="salesdb"/>
    <elementParameter name="TABLE" value="orders_filtered"/>
  </node>
  <connection source="tOracleInput_1" target="tMap_1" connectorName="FLOW" lineStyle="0" label="row1"/>
  <connection source="tMap_1" target="tFilterRow_1" connectorName="FLOW" lineStyle="0" label="row2"/>
  <connection source="tFilterRow_1" target="tPostgresqlOutput_1" connectorName="FLOW" lineStyle="0" label="row3"/>
</talendfile:ProcessType>
